<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 width="100%" height="100%"
		 creationComplete="on_creation_complete(event)"
>
<fx:Script>
<![CDATA[
	import mx.collections.ArrayCollection;
	import mx.events.FlexEvent;
	
[Bindable] private var dp:ArrayCollection;	

protected function on_creation_complete(event:FlexEvent):void
{
	dp = Engine.engine.dep_dp;
}



]]>
</fx:Script>

	
<mx:List width="100%" height="100%" dataProvider="{dp}" labelField="DeptName" itemRenderer="renderers.DeptEditItemRenderer" />	
	
<fx:Declarations>
</fx:Declarations>
</mx:VBox>
